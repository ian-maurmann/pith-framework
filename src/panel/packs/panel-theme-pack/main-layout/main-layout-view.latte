<!DOCTYPE html>
<html lang="en">
    <head>
        <!-- Format -->
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

        <!-- Pith Panel Theme Layout using Latte -->

        <!-- Title -->
        <title>{insertPageTitle()}</title>

        <!-- Viewport -->
        <meta name="HandheldFriendly" content="true">
        <meta name="MobileOptimized"  content="width">
        <meta name="viewport"         content="width=device-width, height=device-height, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scale=no">

        <!-- Page Description -->
        <meta name="description" content="{insertMetaDescription()}">

        <!-- Page Keywords -->
        <meta name="keywords" content="{insertMetaKeywords()}">

        {insertMetaRobots(2)}
        {insertResourceFiles(2)}
    </head>
    <body data-theme="pith-panel" data-color-mode="light">
        {insertPage()}
    </body>
</html>